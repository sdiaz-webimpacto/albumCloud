<div class="col-xs-12">
    <table class="table table-striped">
        <thead>
        <tr>
            <th colspan="3">
                <h3>Todos los albumes a lso que tengo acceso</h3>
            </th>
        </tr>
        <tr>
            <th>Title</th>
            <th>Name</th>
            <th>Banner</th>
        </tr>
        </thead>
        <tbody>
        {% for album in albumes %}
            <tr>
                <td style="width: 25%;">{{ album.title }}</td>
                <td style="width: 50%;">{{ album.getAlbumName() }}</td>
                <td style="width: 25%;"><img src="{{ asset('img/user/') }}{{ album.getAlbumBanner() }}" width="100%" alt="banner"></td>
            </tr>
        {% endfor %}

        </tbody>
    </table>
</div>